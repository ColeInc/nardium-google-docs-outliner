{
    "short_name": "Nardium",
    "name": "Nardium - Google Docs Outline Tool",
    "description": "A Chrome extension to interact with Google Docs and perform actions on headings.",
    "version": "1.0.0",
    "manifest_version": 2,
    "permissions": ["activeTab", "https://www.googleapis.com/*"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Google Docs Headings",
        "default_icon": "logo192.png",
        "default_popup": "index.html"
    },
    "icons": {
        "16": "logo192.png"
    }
}
